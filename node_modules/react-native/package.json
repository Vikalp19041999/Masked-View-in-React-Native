    "test-ci": "jest --maxWorkers=2 --ci --reporters=\"default\" --reporters=\"jest-junit\"",
    "test-ios": "./scripts/objc-test.sh test",
    "test-ios-e2e": "detox test -c ios.sim.release RNTester/e2e"
